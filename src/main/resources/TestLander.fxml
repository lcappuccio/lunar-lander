<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.input.KeyCodeCombination?>
<?import javafx.scene.layout.*?>
<Pane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
      prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="org.systemexception.lunarlander.controller.MainController">
	<children>
		<MenuBar prefHeight="29.0" prefWidth="600.0">
			<menus>
				<Menu mnemonicParsing="false" text="File">
					<items>
						<MenuItem mnemonicParsing="false" text="About">
							<accelerator>
								<KeyCodeCombination alt="UP" code="A" control="UP" meta="ANY" shift="UP" shortcut="UP"/>
							</accelerator>
						</MenuItem>
						<MenuItem mnemonicParsing="false" text="Quit">
							<accelerator>
								<KeyCodeCombination alt="UP" code="Q" control="UP" meta="ANY" shift="UP" shortcut="UP"/>
							</accelerator>
						</MenuItem>
					</items>
				</Menu>
			</menus>
		</MenuBar>
		<Pane layoutY="29.0" prefHeight="375.0" prefWidth="600.0">
			<children>
				<GridPane layoutY="7.0">
					<columnConstraints>
						<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
						<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
					</columnConstraints>
					<rowConstraints>
						<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
						<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
						<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
					</rowConstraints>
					<children>
						<Button mnemonicParsing="false" onAction="#quit" text="Quit" GridPane.columnIndex="0"
						        GridPane.halignment="CENTER"/>
						<Button mnemonicParsing="false" onAction="#start" text="Start" GridPane.columnIndex="1"
						        GridPane.halignment="CENTER"/>
					</children>
				</GridPane>
			</children>
		</Pane>
	</children>
</Pane>
